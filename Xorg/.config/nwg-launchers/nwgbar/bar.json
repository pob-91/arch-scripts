[
  {
    "name": "Lock screen",
    "exec": "bash ~/.config/i3/scripts/blur-lock",
    "icon": "~/.config/nwg-launchers/nwgbar/lock.png"
  },
  {
    "name": "Logout",
    "exec": "logout",
    "icon": "system-log-out"
  },
  {
    "name": "Reboot",
    "exec": "systemctl reboot",
    "icon": "system-reboot"
  },
  {
    "name": "Shutdown",
    "exec": "systemctl -i poweroff",
    "icon": "system-shutdown"
  }
]
